# set-bounds 5 1 #
import f :(unit -> unit)

int reached := 0;
int x := 0;

public run (u:unit) :(unit) = {
  (if !x == 0
  then (x := 1; f(); if !x == 2 then reached := 1 else ())
  else (if !x == 1 then x := 2 else ()));
  assert(not !reached)
};

//  Adapted from Example 11 from 'A contextual equivalence checker for IMJ*'