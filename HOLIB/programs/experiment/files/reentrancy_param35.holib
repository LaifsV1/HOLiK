# set-bounds 3 5 #
import send :(int -> unit)

public withdraw (m:int) :(unit) = {
  if ((m - 1) < !param)
  then send(m);
       balance := !balance - m;
       assert(not(!balance < 0))
  else ()};
int balance := 100;
int param   := 50;

// this is a parametrised reentrancy example
// k:     2   , 3  , 5  , 8
// param: 100 , 50 , 25 , 13